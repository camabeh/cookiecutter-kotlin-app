  "version_lmax_disruptor": "3.4.2",
  "version_gradle": "1.2.10",
  "version_slf4j": "1.7.25",
  "version_log4j2": "2.11.1",
  "version_junit": "4.12",
  "gradle_version": "3.5-rc-2",
  "_copy_without_render": [
    ".gitignore",
    "gradlew",
